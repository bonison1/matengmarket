  employee_id    String?    @db.Uuid
  name           String?
  email          String?
  date           DateTime?  @default(now()) @db.Date
  team           String?
  pickup_name    String?
  pickup_address String?
  pickup_phone   String?
  drop_name      String?
  drop_address   String?
  drop_phone     String?
  orderType      String?
  pb             String?
  dc             String?
