}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model item_datas {
  id                  Int       @id @default(autoincrement())
  name                String    @db.VarChar(255)
  mrp                 Decimal   @db.Decimal(10, 2)
  discount            Decimal?  @default(0) @db.Decimal(10, 2)
  tax                 Decimal?  @default(0) @db.Decimal(10, 2)
  rate_after_discount Decimal?  @default(dbgenerated("(mrp - discount)")) @db.Decimal(10, 2)
  created_at          DateTime? @default(now()) @db.Timestamptz(6)
  updated_at          DateTime? @default(now()) @db.Timestamptz(6)
  user_id             String?   @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  qty                 Decimal?  @db.Decimal
  users               users?    @relation(fields: [user_id], references: [user_id], onDelete: Cascade, onUpdate: NoAction, map: "fk_user_id")

  @@index([name], map: "idx_item_name")
}

